{
  "name": "pepperfm/api-responder-for-laravel",
  "description": "Easy api responder template using via DI",
  "keywords": [
    "pepperfm",
    "laravel",
    "api",
    "json",
    "rest",
    "laravel-api-responder"
  ],
  "homepage": "https://github.com/pepperfm/api-responder-for-lravel",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Dmitry Gaponenko",
      "email": "Damon3453@yandex.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/database": "^10.9.0",
    "illuminate/http": "^10.9.0",
    "illuminate/support": "^10.9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.5",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pepperfm\\ApiBaseResponder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pepperfm\\ApiBaseResponder\\Tests\\": "tests/",
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pepperfm\\ApiBaseResponder\\ApiBaseResponderServiceProvider"
      ],
      "aliases": {
        "ApiBaseResponder": "Pepperfm\\ApiBaseResponder\\BaseResponse"
      }
    }
  }
}
